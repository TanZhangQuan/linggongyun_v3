<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.merchant.mapper.RegulatorTaxDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.merchant.entity.RegulatorTax">
        <id column="id" property="id" />
        <result column="tax_id" property="taxId" />
        <result column="regulator_id" property="regulatorId" />
        <result column="status" property="status" />
        <result column="create_date" property="createDate" />
        <result column="update_date" property="updateDate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, tax_id, regulator_id, status, create_date, update_date
    </sql>

</mapper>
