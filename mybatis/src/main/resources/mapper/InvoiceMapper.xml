<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mybatis.mapper.InvoiceDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.mybatis.entity.Invoice">
        <id column="id" property="id" />
        <result column="merchants_name" property="merchantsName" />
        <result column="pay_code" property="payCode" />
        <result column="pay_detailed_account" property="payDetailedAccount" />
        <result column="gc_pay_receipt" property="gcPayReceipt" />
        <result column="c_payment_receipt" property="cPaymentReceipt" />
        <result column="gc_invoice" property="gcInvoice" />
        <result column="c_invoice" property="cInvoice" />
        <result column="state" property="state" />
        <result column="create_date" property="createDate" />
        <result column="update_date" property="updateDate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, merchants_name, pay_code, pay_detailed_account, gc_pay_receipt, c_payment_receipt, gc_invoice, c_invoice, state, create_date, update_date
    </sql>

</mapper>
